{
    "name": "dation/woocommerce",
    "description": "A Wordpress plugin for synchronizing Dation courses with Woocommerce",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Dation",
            "email": "info@dation.nl"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "setbased/rijksregisternummer": "^0.10.1",
        "ralouphie/getallheaders": "3.0.3 as 2.0.5",
        "symfony/serializer": "^4.3",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^4.3",
        "phpdocumentor/reflection-docblock": "^4.3",
        "viison/address-splitter": "^0.3.2"
    },
    "autoload": {
        "psr-4": {
            "Dation\\Woocommerce\\": "includes/"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.19"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "fzaninotto/faker": "^1.8",
        "roave/security-advisories": "dev-master"
    }
}
